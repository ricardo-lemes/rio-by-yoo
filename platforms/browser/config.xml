<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1002" id="com.criarenet.rio" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Rio</name>
    <description>
        Rio by yoo.
    </description>
    <author email="support@wifimax.com" href="http://www.wifimax.com.br/">
        Wifimax Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:config-file mode="replace" parent="UIDeviceFamily" platform="ios">
        <string>1</string>
    </gap:config-file>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="26" />
</widget>
